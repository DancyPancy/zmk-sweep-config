			/* 												   |			  ||			  |	   |			  ||			  |													*/
																&lt scut SPACE	&mo npad			&mo rnav		&salpha2
			/* 												   ╰──────────────╯╰──────────────╯	   ╰──────────────╯╰──────────────╯													*/
			>;
		};

		/*1*/
		alpha2 {
			bindings = <
